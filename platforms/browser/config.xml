<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.cubetimer" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>SpeedCubeTimer</name>
    <description>
        The #1 app for Rubik's Cube enthusiasts!!!
    </description>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="SplashScreenDelay" value="3000" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="SplashScreen" value="res/screen/android" />
    <preference name="SplashMaintainAspectRatio" value="true" />
</widget>
